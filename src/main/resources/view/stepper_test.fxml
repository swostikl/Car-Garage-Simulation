<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="simu.controller.StepperTestViewController">
   <children>
      <Button fx:id="runButton" defaultButton="true" mnemonicParsing="false" onAction="#onPressRun" text="Run">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <ToggleButton fx:id="pauseButton" mnemonicParsing="false" onAction="#onPausePressed" text="Pause" />
      <Button fx:id="stepButton" disable="true" mnemonicParsing="false" onAction="#onStepPressed" text="Step" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
